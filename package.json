{
  "name": "betcoin-frontend",
  "version": "0.1.0",
  "description": "Betcoin frontend",
  "private": true,
  "scripts": {
    "build":
      "cross-env REACT_APP_VERSION=$npm_package_version react-scripts build",
    "clean": "rimraf build coverage",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "yarn lint-js && yarn lint-format",
    "lint-format": "prettier --list-different \"**/*.{js,json,md}\"",
    "lint-js": "eslint .",
    "start":
      "cross-env REACT_APP_VERSION=$npm_package_version react-scripts start",
    "test":
      "cross-env REACT_APP_VERSION=$npm_package_version react-scripts test --env=jsdom"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "i18next": "^10.6.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.22.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-event-listener": "^0.5.3",
    "react-i18next": "^7.5.1",
    "react-jss": "^8.3.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "prettier": "^1.11.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2"
  }
}
